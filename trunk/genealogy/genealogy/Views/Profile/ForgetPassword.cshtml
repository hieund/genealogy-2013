@{
    ViewBag.Title = "ForgetPassword";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="formlogin" method="post" enctype="multipart/form-data">
    <div class="box">
        <div class="box-heading">
            Quên mật khẩu
        </div>
        <div class="box-content">
            <input type="text" value="Nhập email để chúng tôi gửi thông tin xác " id="email" name="email">
            <button type="submit" class="button"><span>Lấy lại mật khẩu</span></button>
        </div>
    </div>
</form>
