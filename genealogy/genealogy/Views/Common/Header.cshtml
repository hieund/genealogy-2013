@using genealogy.business;
@using genealogy.business.Base;
@using genealogy.business.Custom;
@model System.Text.StringBuilder
<div class="header_top">
    <marquee align="center" direction="left" height="auto" scrollamount="3" width="100%">
					<img src="/Content/themes/images/welcome_03.png"/>
				</marquee>
    <a class="header_logo" href="/" title="">
        <img src="/Content/themes/images/logo_03.png" /></a>
</div>
<div class="header_bottom">
    <div class="menu" id="smoothmenu1">

        <ul>
            <li class="active"><a href="/" class="active">Trang chủ</a> </li>
            @Html.Raw(Model.ToString())
        </ul>

    </div>

    <div class="tab_bottom">
        <div class="social">
            <h3>Liên kết</h3>
            <ul>
                <li class="facebook"><a title="Facebook" href="#">Facebook</a></li>
                <li class="twitter"><a title="Twitter" href="#">Twitter</a></li>
                <li class="rss"><a title="Vimeo" href="#">RSS Feed</a></li>
                <li class="flickr"><a title="Flickr" href="#">Flickr</a></li>
                <li class="vimeo"><a title="RSS Feed" href="#">Vimeo</a></li>
            </ul>
        </div>
        <div id="search">
            <input type="text" onkeydown="this.style.color = '#000000';" onclick="this.value = '';" value="Tìm kiếm" name="filter_name">
            <div title="Search" class="button-search"></div>
        </div>
    </div>
</div>
